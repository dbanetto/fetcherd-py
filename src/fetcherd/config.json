{
    "source": {
        "class": "web_client",
        "settings": {
            "url": "http://localhost:8000/"
        }
    },
    "providers": {
        "blacklist": [],
        "whitelist": []
    },
    "daemon": {
        "pid": "/tmp/fetcherd.pid"
	},
	"fetch": {
		"save_paths": {
			"mp3$": "./podcasts"
		},
		"save_path_default": "./downloads",

		"check_filesystem": true,
		"search_paths": [
			"./podcasts"
		]
	},
	"sort": {
		"save_paths": {
			"rss": "./podcast"
		},
		"save_path_default": "./sorted",
		"search_paths": [
			"./downloads"
		],
		"providers_to_move":[
			"roosterteeth"
		]
	}
}
